                        "firstName" : {
                          "description" : "First name",
                          "type" : "string",
                          "maxLength" : 60
                        },
                        "lastName" : {
                          "description" : "Last name",
                          "type" : "string",
                          "maxLength" : 60
                        },
                        "address" : {
                          "description" : "Address information for Payer",
