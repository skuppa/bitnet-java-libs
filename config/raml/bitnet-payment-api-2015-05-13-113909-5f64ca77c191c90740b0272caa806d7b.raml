"/invoices": 
    description: |
        An invoice is the instruction for the payment which is generated for an order. 
        
        An invoice is required to process a payment in digital currency    
    type: "typedCollection"
    get: 
        description: "Get all invoices, optionally filtered"
        headers: 
            "If-None-Match": 
                description: "The If-None-Match request-header field is used with a method to make it conditional, allowing efficient use of cached information with a minimum amount of transaction overhead."
                type: string
                required: false
                repeat: false
                example: "45322xyzzy"
            "Authorization": 
                description: |
                    A valid OAuth token in format 'Bearer $access_token'
                    
                    To get a valid token use the oAuth resource to create one
                type: string
                required: false
                repeat: false
                example: "Bearer eyJhbGciOiJIUzI1NiJ9..s40N_JAu6rRAQLpGu_KREES4DmFmEPmWEHDrzDJ9zro"
        queryParameters: 
            "accountId": 
                description: "Account that invoices belong to"
                type: string
                required: false
                repeat: false
                example: "821baab2-90e1-a32d-2094-b31da1db6a42"
            "orderId": 
                description: "Order that invoices belong to"
                type: string
                required: false
                repeat: false
                example: "1d001ab9-772f-3942-213d-93144fd851da"
            "paymentAddress": 
                description: "Payment address for a specific invoice"
                type: string
                required: false
                repeat: false
                example: "mvzssahncsZBXmHWSYk7HdAbVwQR8KRVQk"
            "reference": 
                description: "Reference for a specific invoice"
                type: string
                required: false
                repeat: false
                example: "Customer-Order-0000-1234-5678"
            "state": 
                description: "Filter invoices by current state.  Multiple state params can be supplied to filter by multiple states. Is case-sensitive, and must be all UPPERCASE"
                type: string
                required: false
                repeat: true
