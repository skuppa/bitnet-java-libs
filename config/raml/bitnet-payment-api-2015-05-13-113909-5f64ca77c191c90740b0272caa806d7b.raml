                          "properties" : {
                            "addressLine1" : {
                              "description" : "1st line of address",
                              "type" : "string",
                              "minLength" : 1,
                              "maxLength" : 60
                            },
                            "addressLine2" : {
                              "description" : "2nd line of address",
                              "type" : "string",
                              "maxLength" : 60
                            },
